{
  "version": 2,
  "public": true,
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|webp|svg|mp3|wav|ico|woff|woff2|ttf|eot))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 